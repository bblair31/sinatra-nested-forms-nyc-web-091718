<h1>Display your Pirate here</h1>
  <h3>Name: <%= @pirate.name %></h3>
  <h3>Height: <%= @pirate.weight %></h3>
  <h3>Weight: <%= @pirate.height %></h3>

<h2>Ships</h2>
  <% @ships.each do |ship| %>
    <p>Name: <%= ship.name %></p>
    <p>Type: <%= ship.type %></p>
    <p>Booty: <%= ship.booty %></p>
  <% end %>
